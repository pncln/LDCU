FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    aerofoil
    {
        type            compressible::alphatWallFunction;
        value           nonuniform List<scalar> 0();
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    procBoundary8to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary8to6
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary8to7
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary8to9
    {
        type            processor;
        value           uniform 0;
    }
}


