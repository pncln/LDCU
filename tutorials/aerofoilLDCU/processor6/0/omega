FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10;

boundaryField
{
    aerofoil
    {
        type            omegaWallFunction;
        value           nonuniform List<scalar> 0();
    }
    inlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 10;
        value           uniform 10;
    }
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    procBoundary6to2
    {
        type            processor;
        value           uniform 10;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 10;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 10;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 10;
    }
    procBoundary6to8
    {
        type            processor;
        value           uniform 10;
    }
}


