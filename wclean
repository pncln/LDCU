#!/bin/sh
# Clean build artifacts for rhoPimpleCentralFoam
set -e
# Load OpenFOAM v2406 environment if not already loaded
if [ -z "$WM_PROJECT_DIR" ] || [ ! -d "$WM_PROJECT_DIR" ]; then
  if [ -f /Volumes/OpenFOAM-v2406/etc/bashrc ]; then
    . /Volumes/OpenFOAM-v2406/etc/bashrc
  elif [ -f "$HOME/OpenFOAM/OpenFOAM-v2406/etc/bashrc" ]; then
    . "$HOME/OpenFOAM/OpenFOAM-v2406/etc/bashrc"
  fi
fi
cd "$(dirname "$0")/rhoPimpleCentralFoam" || exit 1
/Volumes/OpenFOAM-v2406/wmake/wclean "$@"
